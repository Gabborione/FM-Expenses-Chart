    min-width: 350px;\r\n    height: 100px;\r\n    background-color: $soft-red;\r\n    border-radius: 10px;\r\n    padding: 25px 20px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n\r\n    *{\r\n        color: $very-pale-orange;\r\n    }\r\n}\r\n\r\n.body-container{\r\n    width: 350px;\r\n    width: 350px;\r\n    height: 400px;\r\n    background-color: $very-pale-orange;\r\n    border-radius: 10px;\r\n    padding: 25px 20px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n\r\n.chart{\r\n    width: 100%;\r\n    height: 78%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: flex-start;\r\n    padding-bottom: 1.5rem;\r\n\r\n    .chart__title{\r\n        color: $dark-brown;\r\n        margin-bottom: 1.5rem;\r\n    }\r\n}\r\n\r\nhr{\r\n    width: 100%;\r\n    border-top: 2px solid $cream;\r\n}\r\n\r\n.chart__body{\r\n    width: 100%;\r\n    height: 100%;\r\n    display: grid;\r\n    grid-template-columns: repeat(7, 1fr);\r\n    gap: 0.7rem;\r\n    margin-bottom: 1.5rem;  \r\n\r\n    .day{\r\n        height: 100%;\r\n        width: 100%;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: flex-end;\r\n        align-self: center;\r\n        gap: .8rem;\r\n\r\n        .bar::before{\r\n            content: attr(data-val);\r\n            background-color: $dark-brown;\r\n            border-radius: 4px;\r\n            display: block;\r\n            padding: 0.25rem;\r\n            font-size: 0.7rem;\r\n            opacity: 0;\r\n            transition: opacity 0.25s linear;\r\n            position: absolute;\r\n            top:0;\r\n            left: 50%;\r\n            transform: translate(-50%, -125%);\r\n            z-index: 2;\r\n            color: $very-pale-orange;\r\n        }\r\n\r\n        &:hover .bar::before{\r\n            opacity: 1;\r\n        }\r\n\r\n        &:hover .current{\r\n            background-color: $cyan-light !important;\r\n        }\r\n\r\n        &:hover .bar{\r\n            background-color: $soft-red-light;\r\n        }\r\n\r\n        .bar{\r\n            position: relative;\r\n            height: 70%;\r\n            width: 100%;\r\n            background-color: $soft-red;\r\n            border-radius: 5px;\r\n        }\r\n\r\n        p{\r\n            width: 100%;\r\n            color: $medium-brown;\r\n            text-align: center;\r\n        }\r\n    }\r\n\r\n}\r\n\r\n.expenses{\r\n    width: 100%;\r\n    height: 22%;\r\n\r\n    h3{\r\n        text-align: start;\r\n        color: $medium-brown;\r\n        margin-bottom: 0.1rem;\r\n    }\r\n}\r\n\r\n.info-container{\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n\r\n    h1{\r\n        font-size: 2.2rem;\r\n    }\r\n}\r\n\r\n.percentage-container{\r\n    width: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: flex-end;\r\n\r\n\r\n    .percentage{\r\n        color: $dark-brown;\r\n        font-weight: 700;\r\n    }\r\n}\r\n\r\n.current{\r\n    background-color: $cyan !important;\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"0fd9eace5cffd4899455270d12018f49","cacheData":{"env":{}}}